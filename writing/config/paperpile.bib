  keywords = "DNA/enrichment",
  language = "en"
}

@ARTICLE{Bhullar2012-qs,
  title    = "Antibiotic resistance is prevalent in an isolated cave microbiome",
  author   = "Bhullar, Kirandeep and Waglechner, Nicholas and Pawlowski, Andrew
