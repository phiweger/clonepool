@ARTICLE{Larsbrink2014-yk,
  title    = "A discrete genetic locus confers xyloglucan metabolism in select
              human gut Bacteroidetes",
  author   = "Larsbrink, Johan and Rogers, Theresa E and Hemsworth, Glyn R and
