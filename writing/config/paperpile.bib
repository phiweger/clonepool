@ARTICLE{Wylie2015-im,
  title    = "Enhanced virome sequencing using targeted sequence capture",
  author   = "Wylie, Todd N and Wylie, Kristine M and Herter, Brandi N and
              Storch, Gregory A",
  abstract = "Metagenomic shotgun sequencing (MSS) is an important tool for
              characterizing viral populations. It is culture independent,
