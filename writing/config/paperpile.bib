@ARTICLE{Flombaum2013-be,
  title    = "Present and future global distributions of the marine
              Cyanobacteria Prochlorococcus and Synechococcus",
  author   = "Flombaum, Pedro and Gallegos, Jos{\'e} L and Gordillo, Rodolfo A
